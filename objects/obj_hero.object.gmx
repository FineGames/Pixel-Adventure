            <string>/// Керування гравцем 
switch(keyboard_key)
    {
    case vk_left: if acces_left = true { hspeed = -5;} break; // Якщо є дозвіл, то швидкість вліво -5
    case vk_right: if acces_right = true { hspeed = 5;} break; // Якщо є дозвіл, то швидкість вліво 5
    case vk_down: with(obj_w_block) // До (обєкту платформи), від Y віднімаємо 10
                    {
                    Y=Y-10;
                    } break;
    }
// Віджимання лівої клавіші       
if keyboard_check_released(vk_left) = true 
    {
    hspeed = 0; 
    }
// Віджимання правої клавіші
if keyboard_check_released(vk_right) = true
    {
    hspeed = 0;
    } 
   
// Стрибки 
if keyboard_check_pressed(vk_up) = true &amp;&amp; access_jump &lt; 2 // Якщо клавішу віджато і персонаж ще не стрибав, або стрибав 1 раз
    {
    vspeed = -10;
    gravity = 0.5;
    switch (access_jump) // Світч дозволу на стрибок
        {
        case 0: access_jump = 1; break; // Якщо рівний нулю (ГГ ще не стрибав) то він рівний 1 (перший стрибок)
        case 1: access_jump = 2; break; // Якщо вже раз стрибав, то втрибає другий раз (тому і 2)
        }
    } 
// Віджимання клавіші вниз
if keyboard_check_released(vk_down) 
    {
    with (obj_w_block) // До обєкта платформи
        {
        Y = y; // Він тепер буде знову повівнювати із своїм ігриком а не із зміненийм ігриком
        }
    } 

// Гравітація 
if place_free(x,y) // Якщо перс в повітрі то гравітація є
    {
    draw_collision = false;
    gravity = 0.5;
    } 

// Зміна анімації
if direction&gt;0 &amp;&amp; direction&lt;=90
    {
    Direction = 1
    }
else if direction&gt;90 &amp;&amp; direction&lt;180
    {
    Direction = 2
    }
else if direction&gt;180 &amp;&amp; direction&lt;270
    {
    Direction = 3
    }
else if direction&gt;270 &amp;&amp; direction&lt;359
    {
    Direction = 4
    }
switch (direction)
    {
    case 0: Direction = 5 break;
    case 180: Direction = 6 break;
    }

switch (Direction)
    {
    case 1: sprite_index = spr_h_jump_right; image_speed = 0.5 break;
    case 2: sprite_index = spr_h_jump_left; image_speed = 0.5 break;
    case 3: sprite_index = spr_h_fall_left; image_speed = 0.5 break;
    case 4: sprite_index = spr_h_fall_right; image_speed = 0.5 break;
    case 5: 
        if hspeed &gt; 0
            {
            sprite_index = spr_h_right
            image_speed = 0.5
            } 
        else
            {
            sprite_index = spr_h_wait_right
            }
        break;
    case 6: 
        if hspeed &lt; 0 
            {
            sprite_index = spr_h_left 
            image_speed = 0.5
            }
            break;
    } 
    
// Керування звуком
